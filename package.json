{
	"scripts": {
		"css": "sass sass:css sass/style.scss:style.css --style=compressed --no-source-map",
		"watch": "sass sass:css sass/style.scss:style.css --style=compressed --no-source-map --watch",
		"sync": "browser-sync start --proxy \"localhost/fptschool\" --files \"style.css\" \"css/*.css\"",
		"start": "npm run watch",
		"replace": "npm i & node replace.js"
	},
	"devDependencies": {
		"sass": "^1.49.10",
		"replace-in-file": "^6.3.5"
	}
}